    {
      "when": {
        "level": "2"
      },
      "apply": {
        "model": "animal_feeding_trough:block/feeding_trough_content_2"
      }
    },
    {
      "when": {
        "level": "3"
      },
      "apply": {
        "model": "animal_feeding_trough:block/feeding_trough_content_3"
      }
    },
    {
      "when": {
        "level": "4"
      },
      "apply": {
